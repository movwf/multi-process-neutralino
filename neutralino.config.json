{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*", 
    "filesystem.*",
    "os.*",
    "events",
    "events.*"
  ],
  "modes": {
    "window": {
      "title": "multi-process",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/www/icon.png"
    }
  },
  "cli": {
    "frontendLibrary": {
      "patchFile": "/src/index.html",
      "devUrl": "http://localhost:3000"
    },
    "extensionsPath": "/extensions/",
    "resourcesPath": "/src/dist/",
    "clientLibrary": "/src/__neutralino_globals.js",
    "binaryName": "multi-process",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}
